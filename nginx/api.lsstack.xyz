server {
    listen  80;

    location /hls {
        # Serve HLS fragments
        types {
            application/vnd.apple.mpegurl m3u8;
            video/mp2t ts;
        }
        root /tmp;
        add_header Cache-Control no-cache;
    }
    
    location / {
        proxy_pass http://api.lsstack.xyz:3333;
    }
}